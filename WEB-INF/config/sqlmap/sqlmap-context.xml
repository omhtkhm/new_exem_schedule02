<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

	<settings useStatementNamespaces="true" enhancementEnabled="true"
		maxTransactions="50" maxRequests="100" maxSessions="100" />
	<!-- 공통쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/common/CommonSQL.xml" />
	<!-- Login쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/login/LoginSQL.xml" />
	<!-- Mypage쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/mypage/MypageSQL.xml" />
	<!-- Customer쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/customer/CustomerSQL.xml" />
	<!-- department쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/department/DeptSQL.xml" />
	<!-- dbms쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/dbms/DbmsSQL.xml" />
	<!-- team쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/team/TeamSQL.xml" />
	<!-- member쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/member/MemberSQL.xml" />
	<!-- Position 쿼리(직급) -->
	<sqlMap resource="/com/exem9/lms/sqlMap/position/PosiSQL.xml" />
	<!-- Category 쿼리(일정 스케쥴 유형) -->
	<sqlMap resource="/com/exem9/lms/sqlMap/category/CateSQL.xml" />
	<!-- 일정 스케쥴 목록 조회 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/schedule/SchSQL.xml" />
	<!-- 유지보스 관리 쿼리 -->
	<sqlMap resource="/com/exem9/lms/sqlMap/maintenance/MatSQL.xml" />
</sqlMapConfig>
 